{
  "name": "feelsbox-server",
  "version": "1.0.0",
  "description": "Server for Feelsbox",
  "main": "app/index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "npm run clean && npm run build-server",
    "build-server": "./node_modules/.bin/babel -d ./build ./app --copy-files",
    "clean": "rm -rf build && mkdir build",
    "start": "./node_modules/.bin/nodemon --inspect app/index.js",
    "serve": "node ./build/index.js",
    "import": "./node_modules/.bin/nodemon --inspect .bin/import.js",
    "updateCategories": "./node_modules/.bin/nodemon --inspect .bin/updateCategories.js"
  },
  "author": "Joel Watson",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/node": "^7.8.7",
    "@babel/preset-env": "^7.9.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "connect-flash": "^0.1.1",
    "eslint": "^7.18.0",
    "eslint-plugin-class-property": "^1.1.0",
    "nodemon": "^2.0.2"
  },
  "dependencies": {
    "@babel/polyfill": "^7.8.7",
    "apollo-datasource": "^0.7.0",
    "apollo-server-express": "^2.11.0",
    "aws-sdk": "^2.816.0",
    "bristol": "^0.4.0",
    "canvas": "^2.6.1",
    "connect-mongo": "^3.2.0",
    "cors": "^2.8.5",
    "crypto-random-string": "^3.2.0",
    "dataloader": "^2.0.0",
    "express": "^4.17.1",
    "express-session": "^1.17.0",
    "fs-extra": "^9.0.0",
    "graphql": "^14.6.0",
    "graphql-passport": "^0.6.3",
    "hex-and-rgba": "^1.4.2",
    "lodash": "^4.17.15",
    "mongodb": "^3.5.5",
    "mongoose": "^5.9.6",
    "palin": "^2.3.0",
    "passport": "^0.4.1",
    "passport-facebook": "^3.0.0",
    "passport-google-oauth20": "^2.0.0",
    "socket.io": "^2.3.0",
    "uuid": "^8.3.2",
    "web-push": "^3.4.4"
